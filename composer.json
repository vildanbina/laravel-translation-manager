{
  "name": "vildanbina/laravel-translation-manager",
  "description": "Manage Laravel Translations",
  "keywords": [
    "laravel",
    "translations",
    "translator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vildan Bina",
      "email": "vildanbina@gmail.com"
    }
  ],
  "require": {
    "php": ">=7",
    "illuminate/support": "^7.15",
    "illuminate/translation": "^7.15",
    "symfony/finder": "^5.1",
    "tanmuhittin/laravel-google-translate": "^1.0.2"
  },
  "autoload": {
    "psr-4": {
      "vildanbina\\TranslationManager\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5-dev"
    },
    "laravel": {
      "providers": [
        "vildanbina\\TranslationManager\\ManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
